{
  "name": "data-source",
  "version": "1.1.1",
  "description": "data-source for stock data",
  "main": "lib/index.js",
  "scripts": {
    "build": "BABEL_ENV=babel ../../node_modules/.bin/babel src -d lib",
    "test": "BABEL_ENV=ava ../../node_modules/.bin/nyc ../../node_modules/.bin/ava --verbose --timeout=10s",
    "prepublish": "npm run build",
    "report-cov": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "files": [
    "lib/",
    "src/"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kaelzhang/compton.git"
  },
  "keywords": [
    "data-source"
  ],
  "engines": {
    "node": ">=0.10.0"
  },
  "ava": {
    "require": "babel-register",
    "babel": {
      "babelrc": true
    },
    "files": [
      "test/*.js"
    ]
  },
  "author": "kaelzhang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaelzhang/soros/issues"
  },
  "devDependencies": {
    "data-source-loader-qq": "^1.1.1"
  },
  "dependencies": {
    "knex": "^0.13.0",
    "layered-cache": "^1.1.0",
    "lru-cache": "^4.0.2",
    "mysql": "^2.14.1",
    "ready-queue": "^1.0.0",
    "skema": "^5.0.1",
    "time-spans": "^3.0.0"
  }
}
